<div class="skill-pp">
    {% if resource.skills is not empty %}
        {% for item in resource.skills %}
            <div style="width:300px;height:50px;background-color: black; margin-bottom: 25px;">
                <div style="width:{{ item.level }}%;height:50px;background-color: yellow;">
                    {{ item.name }}
                </div>
            </div>
        {% endfor %}
    {% endif %}
</div>